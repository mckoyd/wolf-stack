{
  "name": "wolf-stack",
  "version": "1.0.0",
  "description": "An implementation of a stack data structure",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "nodemon --exec babel-node index.js",
    "build": "npm run clean && npm run build:src",
    "build:src": "babel src --out-dir dist",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mckoyd/wolf-stack.git"
  },
  "author": "Dominick McKoy <dominick.mckoy@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mckoyd/wolf-stack/issues"
  },
  "homepage": "https://github.com/mckoyd/wolf-stack#readme",
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "babel-preset-es2015": "6.24.1",
    "jest": "24.9.0",
    "nodemon": "2.0.2"
  }
}
